# =============================
# World map data (Bangladesh)
# =============================
library(sf)               # for spatial operations
library(rnaturalearth)    # for world map data
world_countries <- ne_countries(scale = "medium", returnclass = "sf")   # country polygons
bangladesh <- world_countries[world_countries$admin == "Bangladesh", ]  # Bangladesh polygon
world_oceans <- ne_download(
scale = "medium",
type = "ocean",
category = "physical",
returnclass = "sf"
)   # ocean polygons
# =============================
# Projection (Robinson)
# =============================
target_crs_robinson <- "ESRI:54030"   # Robinson projection CRS
bangladesh_robinson <- st_transform(bangladesh, crs = target_crs_robinson)      # project Bangladesh
world_oceans_robinson <- st_transform(world_oceans, crs = target_crs_robinson)  # project oceans
# crop oceans around Bangladesh extent (optional but neat)
world_oceans_bd_robinson <- st_crop(
world_oceans_robinson,
st_bbox(bangladesh_robinson)
)   # keep ocean near Bangladesh
# =============================
# Load & prepare station data (Bangladesh)
# =============================
library(dplyr)            # data cleaning
library(readr)            # CSV reading
stations <- read_csv(
"C:/Users/user/Downloads/Weather-Stations-Map-main/Weather-Stations-Map-main/isd-history.csv",
show_col_types = FALSE
)   # read station file
stations_clean <- stations %>%
filter(!is.na(LAT), !is.na(LON)) %>%                 # remove missing coords
filter(LAT >= -90, LAT <= 90,                        # valid latitude
LON >= -180, LON <= 180)                      # valid longitude
stations_sf <- st_as_sf(
stations_clean,
coords = c("LON", "LAT"),
crs = 4326,
remove = FALSE                                       # keep columns
)   # convert to sf
stations_sf_robinson <- st_transform(stations_sf, crs = target_crs_robinson)   # reproject stations
# keep only stations inside Bangladesh
stations_bd_robinson <- st_join(
stations_sf_robinson,
bangladesh_robinson,
join = st_within,
left = FALSE
)   # stations within Bangladesh polygon
# =============================
# Plot map (Bangladesh)
# =============================
library(ggplot2)          # plotting
ggplot() +
geom_sf(data = world_oceans_bd_robinson, fill = "lightblue", color = NA) +
geom_sf(data = bangladesh_robinson, fill = "grey90", color = "grey60", linewidth = 0.3) +
geom_sf(data = stations_bd_robinson, size = 0.5, color = "red", alpha = 0.7) +
theme_minimal() +
theme(
axis.text = element_blank(),
axis.title = element_blank(),
panel.grid = element_blank()
# text = element_text(family = "Roboto")   # optional font
)
